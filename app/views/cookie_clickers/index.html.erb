<h1>CookieClicker#index</h1>
<p>Find me in app/views/cookie_clicker/index.html.erb</p>
<% if current_model.cookie_clicker == 0%>
    <%= link_to "Start New Cookie Adventure", new_cookie_clicker_path, class:'btn btn-outline-primary'%>
<% else %>
    <%= link_to "Load in Cookie Adventure", cookie_clicker_path(current_model.cookie_clicker), class:'btn btn-outline-primary'%>
<% end %>
